<p style="color: green"><%= notice %></p>
<div class="container">
  <h1>Announcements</h1>
  <%= will_paginate %>
  <div>
      <% @announcements.each do |announcement|  %>
          <%= render announcement %>
          <p>
            <%= link_to "Show", announcement%>
          </p>
      <% end %>
  </div>
  <%= will_paginate %>
  <div>
      <%= link_to "New Announcement", new_announcement_path %>
  </div>
</div>